{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "https_port": 443,
  "Kestrel": {
    "Endpoints": {
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://localhost:443",
        "Certificate": {
          "Path": "/app/certs/fullchain.pem",
          "Password": "/app/certs/privkey.pem"
        }
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/app/certs/fullchain.pem",
        "Password": "/app/certs/privkey.pem"
      }
    }
  }
}
